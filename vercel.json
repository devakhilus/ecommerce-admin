{
    "version": 2,
    "framework": null,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.3"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://yourproductionurl.com",
  
      "CACHE_DRIVER": "array",
      "SESSION_DRIVER": "cookie",
      "LOG_CHANNEL": "stderr"
    }
  }
  